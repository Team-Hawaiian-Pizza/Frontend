{
  "rewrites": [
    {
      "source": "/api/chat/(.*)",
      "destination": "http://15.165.220.74:8000/api/chat/$1"
    },
    {
      "source": "/api/ai/(.*)",
      "destination": "http://3.37.82.219/api/ai/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "http://13.124.106.69:8000/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}